<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.200" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="bgunics-Talend" RELEASE_DATE="2020511A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

  <FAMILIES>
    <FAMILY>Internet/Samba</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
	
	
	    <PARAMETER NAME="HOST" FIELD="TEXT" REQUIRED="true" NUM_ROW="1">
      <DEFAULT>"192.168.0.1"</DEFAULT>
    </PARAMETER>
    
        <PARAMETER NAME="DOMAIN" FIELD="TEXT" REQUIRED="true" NUM_ROW="1">
      <DEFAULT>"domain"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true" NUM_ROW="2">
      <DEFAULT>"user"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="PASSWORD" FIELD="PASSWORD" REQUIRED="true" NUM_ROW="2">
      <DEFAULT>"pass"</DEFAULT>
    </PARAMETER>
		<PARAMETER NAME="REMOTEDIRECTORY" FIELD="REMOTEDIRECTORY" NUM_ROW="3">
			<DEFAULT>"SharedDirectory"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="LIST_MODE" FIELD="CLOSED_LIST" NUM_ROW="4">
			<ITEMS DEFAULT="FILES">
				<ITEM NAME="FILES" VALUE="FILES" />
				<ITEM NAME="DIRECTORIES" VALUE="DIRECTORIES" />
				<ITEM NAME="BOTH" VALUE="BOTH" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="INCLUDSUBDIR" FIELD="CHECK" NUM_ROW="5">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CASE_SENSITIVE" FIELD="CLOSED_LIST"
			NUM_ROW="4">
			<ITEMS DEFAULT="YES">
				<ITEM NAME="YES" VALUE="YES" />
				<ITEM NAME="NO" VALUE="NO" />
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER NAME="ERROR" FIELD="CHECK" NUM_ROW="4">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="GLOBEXPRESSIONS" FIELD="CHECK" NUM_ROW="5">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

	 	<PARAMETER NAME="FILES" FIELD="TABLE" REQUIRED="false" NUM_ROW="6" NB_LINES="5">
     		<ITEMS>
     	   		<ITEM NAME="FILEMASK" />
     	 	</ITEMS>
    	</PARAMETER>

	 	 <!-- order by -->
	 	<PARAMETER NAME="ORDER_BY_NOTHING" FIELD="RADIO" NUM_ROW="11" GROUP="ORDER_BY">
	 		<DEFAULT>true</DEFAULT>
	 	</PARAMETER>
	 	<PARAMETER NAME="ORDER_BY_FILENAME" FIELD="RADIO" NUM_ROW="11" GROUP="ORDER_BY">
	 		<DEFAULT>false</DEFAULT>
	 	</PARAMETER>
	 	<PARAMETER NAME="ORDER_BY_FILESIZE" FIELD="RADIO" NUM_ROW="11" GROUP="ORDER_BY">
	 		<DEFAULT>false</DEFAULT>
	 	</PARAMETER>
	 	<PARAMETER NAME="ORDER_BY_MODIFIEDDATE" FIELD="RADIO" NUM_ROW="11" GROUP="ORDER_BY">
	 		<DEFAULT>false</DEFAULT>
	 	</PARAMETER>
	 	
	 	<!-- order action -->
	 	<PARAMETER NAME="ORDER_ACTION_ASC" FIELD="RADIO" NUM_ROW="21" GROUP="ORDER_ACTION" NOT_SHOW_IF="ORDER_BY_NOTHING=='true'">
	 		<DEFAULT>true</DEFAULT>
	 	</PARAMETER>
	 	<PARAMETER NAME="ORDER_ACTION_DESC" FIELD="RADIO" NUM_ROW="21" GROUP="ORDER_ACTION" NOT_SHOW_IF="ORDER_BY_NOTHING=='true'">
	 		<DEFAULT>false</DEFAULT>
	 	</PARAMETER>

    </PARAMETERS>
	
		
	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="IFEXCLUDE" FIELD="CHECK" NUM_ROW="1">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="EXCLUDEFILEMASK" FIELD="TEXT" NUM_ROW="3" SHOW_IF="IFEXCLUDE=='true'">
			<DEFAULT>"*.txt"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="FORMAT_FILEPATH_TO_SLASH" FIELD="CHECK" NUM_ROW="4">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
				<PARAMETER NAME="USE_PROXY" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="85" REPOSITORY_VALUE="USE_PROXY">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	
		<PARAMETER NAME="PROXY_HOST" FIELD="TEXT" 
			NUM_ROW="86" SHOW_IF="USE_PROXY == 'true'" REPOSITORY_VALUE="PROXY_HOST">
			<DEFAULT>"61.163.92.4"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_PORT" FIELD="TEXT"
			NUM_ROW="87" SHOW_IF="USE_PROXY == 'true'" REPOSITORY_VALUE="PROXY_PORT">
			<DEFAULT>"1080"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="PROXY_USERNAME" FIELD="TEXT" 
			NUM_ROW="88" SHOW_IF="USE_PROXY == 'true'" REPOSITORY_VALUE="PROXY_USERNAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_PASSWORD" FIELD="PASSWORD"
			NUM_ROW="89" SHOW_IF="USE_PROXY == 'true'" REPOSITORY_VALUE="PROXY_PASSWORD">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="JCIFS_CONFIG" FIELD="TABLE" NUM_ROW="100" REPOSITORY_VALUE="JCIFS_CONFIG">
      		<ITEMS>
        		<ITEM NAME="KEY" />
        		<ITEM NAME="VALUE" />
      		</ITEMS>
    	</PARAMETER>
    <PARAMETER
      NAME="ENABLE_DEBUG_MODE"
      FIELD="CHECK"
      NUM_ROW="5">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
	</ADVANCED_PARAMETERS>
	<CODEGENERATION>
      <IMPORTS>
		<IMPORT NAME="jakarta-oro" MODULE="jakarta-oro-2.0.8.jar" MVN="mvn:org.talend.libraries/jakarta-oro-2.0.8/6.0.0/jar" REQUIRED="true" />
		<IMPORT NAME="jcifs-2.1.16" MODULE="jcifs-2.1.16.jar" MVN="mvn:org.codelibs/jcifs/2.1.16" REQUIRED="true" />
		<IMPORT NAME="bcprov-jdk15on-1.64" MODULE="bcprov-jdk15on-1.64.jar" MVN="mvn:org.bouncycastle/bcprov-jdk15on/1.64" REQUIRED="true" />
		<IMPORT NAME="slf4j-api" MODULE="slf4j-api-1.7.25.jar" MVN="mvn:org.slf4j/slf4j-api/1.7.25/jar" REQUIRED="true" />
		<IMPORT NAME="slf4j-log4j12" MODULE="slf4j-log4j12-1.7.25.jar" MVN="mvn:org.slf4j/slf4j-log4j12/1.7.25/jar" REQUIRED="true" />
      </IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="CURRENT_FILE" TYPE="id_String" AVAILABILITY="FLOW" />
		<RETURN NAME="CURRENT_FILEPATH" TYPE="id_String" AVAILABILITY="FLOW" />
		<RETURN NAME="CURRENT_FILEEXTENSION" TYPE="id_String" AVAILABILITY="FLOW" />
		<RETURN NAME="CURRENT_FILEDIRECTORY" TYPE="id_String" AVAILABILITY="FLOW" />
		<RETURN NAME="NB_FILE" TYPE="id_Integer" AVAILABILITY="FLOW" />
	</RETURNS>
</COMPONENT>
