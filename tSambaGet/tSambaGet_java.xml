<COMPONENT>
  <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.210" STATUS="ALPHA"
    COMPATIBILITY="ALL" AUTHOR="bgunics-Talend" RELEASE_DATE="2020511A"
    STARTABLE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Internet/Samba</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
  
    <PARAMETER NAME="HOST" FIELD="TEXT" REQUIRED="true" NUM_ROW="10">
      <DEFAULT>"192.168.1.1"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="DOMAIN" FIELD="TEXT" REQUIRED="true" NUM_ROW="15">
      <DEFAULT>"domain"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true" NUM_ROW="20">
      <DEFAULT>"user"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="PASSWORD" FIELD="PASSWORD" REQUIRED="true" NUM_ROW="20">
      <DEFAULT>"pass"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="USE_CACHE" FIELD="CHECK" NUM_ROW="21">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="LOCALFILE" FIELD="FILE" REQUIRED="false" NUM_ROW="30" SHOW_IF="USE_CACHE=='false'">
      <DEFAULT>"/home/nuno/Desktop/titi.txt"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="CREATE_DIRECTORY" FIELD="CHECK" REQUIRED="true" NUM_ROW="40" 
          REPOSITORY_VALUE="CREATE_DIRECTORY" SHOW_IF="USE_CACHE=='false'">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="SHAREDIR" FIELD="TEXT" REQUIRED="false" NUM_ROW="50">
      <DEFAULT>"sharedirectory"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="REMOTEFILE" FIELD="TEXT" REQUIRED="false" NUM_ROW="60">
      <DEFAULT>"tata.txt"</DEFAULT>
    </PARAMETER>
  </PARAMETERS>
  
	<ADVANCED_PARAMETERS>
	
		<PARAMETER NAME="USE_PROXY" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="85" REPOSITORY_VALUE="USE_PROXY">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	
		<PARAMETER NAME="PROXY_HOST" FIELD="TEXT" 
			NUM_ROW="86" SHOW_IF="USE_PROXY == 'true'" REPOSITORY_VALUE="PROXY_HOST">
			<DEFAULT>"61.163.92.4"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_PORT" FIELD="TEXT"
			NUM_ROW="87" SHOW_IF="USE_PROXY == 'true'" REPOSITORY_VALUE="PROXY_PORT">
			<DEFAULT>"1080"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="PROXY_USERNAME" FIELD="TEXT" 
			NUM_ROW="88" SHOW_IF="USE_PROXY == 'true'" REPOSITORY_VALUE="PROXY_USERNAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_PASSWORD" FIELD="PASSWORD"
			NUM_ROW="89" SHOW_IF="USE_PROXY == 'true'" REPOSITORY_VALUE="PROXY_PASSWORD">
			<DEFAULT>""</DEFAULT>
		</PARAMETER> 	  
	  <PARAMETER NAME="JCIFS_CONFIG" FIELD="TABLE" NUM_ROW="100" REPOSITORY_VALUE="JCIFS_CONFIG">
      <ITEMS>
        <ITEM NAME="KEY" />
        <ITEM NAME="VALUE" />
      </ITEMS>
    </PARAMETER>
    <PARAMETER
      NAME="ENABLE_DEBUG_MODE"
      FIELD="CHECK"
      NUM_ROW="5">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
	</ADVANCED_PARAMETERS>
  
  <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="jcifs-2.1.16" MODULE="jcifs-2.1.16.jar" MVN="mvn:org.codelibs/jcifs/2.1.16" REQUIRED="true" />
       <IMPORT NAME="bcprov-jdk15on-1.64" MODULE="bcprov-jdk15on-1.64.jar" MVN="mvn:org.bouncycastle/bcprov-jdk15on/1.64" REQUIRED="true" />
       <IMPORT NAME="slf4j-api" MODULE="slf4j-api-1.7.25.jar" MVN="mvn:org.slf4j/slf4j-api/1.7.25/jar" REQUIRED="true" />
       <IMPORT NAME="slf4j-log4j12" MODULE="slf4j-log4j12-1.7.25.jar" MVN="mvn:org.slf4j/slf4j-log4j12/1.7.25/jar" REQUIRED="true" />
     </IMPORTS>
   </CODEGENERATION>
  <RETURNS>
  	 <RETURN NAME="REMOTEFILEPATH" TYPE="id_String" AVAILABILITY="AFTER"/>
  	 <RETURN NAME="LOCALFILEPATH" TYPE="id_String" AVAILABILITY="AFTER"/>
     <RETURN NAME="INPUT_STREAM" TYPE="java.io.InputStream" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
